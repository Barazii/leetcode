    "class Solution:\n",
    "    def firstMissingPositive(self, nums: List[int]) -> int:\n",
    "        if 1 not in nums:\n",
    "            return 1\n",
    "        nums.sort()\n",
    "        for i in nums:\n",
    "            if i + 1 not in nums and i + 1 > 0:\n",
    "                return i + 1"
   ]
